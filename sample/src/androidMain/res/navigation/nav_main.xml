    <fragment
        android:id="@+id/overlayTestFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.test.OverlayTestFragment" />
    <action
        android:id="@+id/action_global_overlayTestFragment"
        app:destination="@id/overlayTestFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />
</navigation>